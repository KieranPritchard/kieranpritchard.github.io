---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">{% for page in site.pages %}{% if page.layout != null and page.layout != 'feed' and page.layout != 'null' %}
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}{{ page.url | remove: '.html' }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.8</priority>
    <changefreq>weekly</changefreq>
  </url>{% endif %}{% endfor %}{% for post in site.posts %}
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}{{ post.url }}</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <priority>0.6</priority>
    <changefreq>monthly</changefreq>
  </url>{% endfor %}{% for portfolio in site.portfolio %}
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}{{ portfolio.url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.7</priority>
    <changefreq>monthly</changefreq>
  </url>{% endfor %}
</urlset>
